#!/bin/bash

for file in *.o; do
    if [ -f "${file}" ]; then
        echo "Выполняем: readelf -a \"${file}\" > \"${file}.map\""
        nice -n 20 readelf -a "${file}" > "${file}.map" 2>&1
    fi
done

for file in *.axf; do
    if [ -f "${file}" ]; then
        echo "Выполняем: readelf -a \"${file}\" > \"${file}.map\""
        nice -n 20 readelf -a "${file}" > "${file}.map" 2>&1
    fi
done

for file in *.o.IDE; do
    if [ -f "${file}" ]; then
        echo "Выполняем: readelf -a \"${file}\" > \"${file}.map\""
        nice -n 20 readelf -a "${file}" > "${file}.map" 2>&1
    fi
done
